for i in range(2023, 10 ** 10, 2023):
    num = str(i)
    if num[0] == '1' and num[2:6] == '2139' and num[-1] == '4':
        print(i, i // 2023)
---------------------------------------------
from fnmatch import *
for i in range(1022420, 1921399995, 3052):
    if fnmatch(str(i), '1?2139*4'):
        print(i, i // 3052)
