for n in range(1, 100):
    r = bin(n)[2:]
    # строим двоичную запись числа
    r += r[-1]
    # приписываем к числу его последнюю цифру
    if r.count('1') % 2 == 0:
        r += '0'
    else:
        r += '1'
    if r.count('1') % 2 == 0:
        r += '0'
    else:
        r += '1'
    # бит четности по условию (2 раза)
    r = int(r, 2)
    # в десятичную систему и проверка
    if r > 97:
        print(n)
    # исходное число
        break

