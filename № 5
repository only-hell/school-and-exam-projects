for n in range(1, 100):
    r = bin(n)[2:]
    # строим двоичную запись числа
      
    r = int(r, 2)
    # в десятичную систему и проверка
    if r > 97:
        print(n)
    # исходное число
        break
-------------------------------------------------
    s = bin(n)[2:].zfill(8)
    # zfill делает так чтобы длина строки была 8 как у последнего
   
        
