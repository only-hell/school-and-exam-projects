print('x y z')
# выводим имена перемнных
for x in range(2):
    for y in range(2):
        for z in range(2):
            if (not x and y and z) or (not x and not y and z) or (not x and not y and not z) == True:
                # пишем логическую функцию
                # если в таблице истинность F равно 1, то выражение равно True, иначе False
                print(x, y, z)
                # после сравниваем значения из полученной таблицы с данными из условия
