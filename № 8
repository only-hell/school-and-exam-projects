letters = 'abcd'
s = set()
# количество циклов равно количеству букв в слове
for i1 in letters:
    for i2 in letters:
        for i3 in letters:
            for i4 in letters:
               for i5 in letters:
                   word = i1 + i2 + i3 + i4 + i5
                   # в слове только 1 вхождение Х
                   if word.count('d') == 1:
                       s.add(word)
print(len(s))
