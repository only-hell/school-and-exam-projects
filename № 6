from turtle import *
speed(10000)
k = 50
left(90)
for i in range(16):
    left(36)
    forward(4 * k)
    left(36)
    # цикл движений черепахи
penup()
# черепаха не рисует
for x in range(-10, 10):
    for y in range(-10, 10):
        goto(x * k, y * k)
        dot(4)
        # создание точек по ДСК размером 4 пикс

done()
# методы Turtle:
# forward() / backward() - движение черепахи
# back() / right() / left() - изменение напрвления
# pendown() / penup() - поднять / опустить хвост
